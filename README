To run this app you need to have docker with mongodb (at port 27017) and redis (at port 6379) images


# indices = {
#     "NASDAQ": "^IXIC",
#     "DAX30": "^GDAXI",
#     "WIG20": "^WIG20"
# }

#Sample Requests:

    #Get data
    http://127.0.0.1:5000/data/^IXIC?period=10y&interval=1d
    http://127.0.0.1:5000/data/AAPL?period=2y&interval=1h

    #Depracated
    http://127.0.0.1:5000/data/AAPL?start=2014-01-01&end=2015-01-01&interval=1h

    #Prediction
    http://127.0.0.1:5000/predict/AAPL?period=2y&interval=1h&days_ahead=3d


# url = 'https://www.alphavantage.co/query?function=TIME_SERIES_INTRADAY&symbol=IBM&interval=5min&apikey=demo'